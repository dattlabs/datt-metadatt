#!/bin/bash

# Make the script exit on any errors returned by individual commands.
set -e

build_generator() {
  sed "s/METADATT_PROJNAME/$PROJNAME/g" "$DIR/templates/build.template" > "$DIR/containers/$daproject/build"
  # make the build script executable...
  chmod +x "$DIR/containers/$daproject/build"
  echo "[OK] $daproject/build created"
}
